{
  "name": "wibson-tokens-distribution",
  "version": "0.1.0",
  "private": true,
  "author": "Wibson Development Team",
  "scripts": {
    "compile": "truffle compile",
    "ganache": "ganache-cli",
    "lint:all": "npm run lint:js; npm run lint:sol",
    "lint:all:fix": "npm run lint:js:fix; npm run lint:sol:fix",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "lint:sol": "solium -d .",
    "lint:sol:fix": "solium -d . --fix",
    "test": "scripts/test.sh",
    "test:coverage": "SOLIDITY_COVERAGE=true scripts/test.sh",
    "test:inspect": "INSPECT=true scripts/test.sh",
    "truffle": "truffle"
  },
  "devDependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "eslint": "^5.2.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "ganache-cli": "^6.1.6",
    "solidity-coverage": "^0.5.5",
    "solium": "^1.1.8",
    "solium-plugin-zeppelin": "0.0.2",
    "truffle": "^4.1.13",
    "truffle-hdwallet-provider": "0.0.6"
  },
  "dependencies": {
    "openzeppelin-solidity": "^1.11.0"
